  "assembly": {
    "mnemonics": {
      "push": ["push", "push <number>", "push <string>"],
      "dup": ["dupl"],
      "copy": ["copy"],
      "swap": ["swap"],
      "drop": ["pop"],
      "slide": ["slide"],
      "add": ["add"],
      "sub": ["sub"],
      "mul": ["mul"],
      "div": ["div"],
      "mod": ["mod"],
      "store": ["store"],
      "retrieve": ["get"],
      "label": ["label"],
      "call": ["call"],
      "jmp": ["jump"],
