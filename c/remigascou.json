      "dup": ["dup"],
      "swap": ["swap"],
      "drop": ["pop"],
      "add": ["add"],
      "sub": ["sub"],
      "mul": ["mul"],
      "div": ["div"],
      "mod": ["mod"],
      "store": ["store"],
      "retrieve": ["load"],
      "label": ["lbl"],
      "call": ["call"],
      "jmp": ["jmp"],
      "jz": ["jmpz"],
      "jn": ["jmpn"],
      "ret": ["leave"],
      "end": ["end"],
      "printc": ["printc"],
      "printi": ["printn"],
      "readc": ["readc"],
      "readi": ["readn"]
    },
    "line_comments": [";"],
    "usage": ["syntax highlighter"],
    "extension": "asm"
  },
  "programs": [
    { "path": "testprograms/full.ws" },
    { "path": "testprograms/ws/full.ws" },
